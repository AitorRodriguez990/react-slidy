{
  "name": "react-slidy",
  "version": "1.0.2",
  "main": "lib/",
  "keywords": [
    "slider",
    "react",
    "lory",
    "slidy",
    "react-lory",
    "react-slidy"
  ],
  "scripts": {
    "clean:lib": "rimraf ./lib/*",
    "prelib": "npm run clean:lib -s",
    "dist": "webpack",
    "lib": "npm run lib:scripts && npm run lib:styles",
    "lib:scripts": "babel src --out-dir lib",
    "lib:styles": "copyfiles -u 1 './src/**/*.scss' lib",
    "prepublish": "npm run lib",
    "lint": "npm run lint",
    "start": "npm run start:open && npm run start:server",
    "start:server": "webpack-dev-server",
    "start:open": "opener http://localhost:3000",
    "lint:js": "node_modules/.bin/eslint --ext .js,.jsx src/",
    "lint:sass": "node_modules/.bin/sass-lint src/**/*.scss -c -v"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:miduga/react-slidy.git"
  },
  "pre-commit": [
    "lint",
    "test",
    "dist"
  ],
  "peerDependencies": {
    "react": "0.14 || 15"
  },
  "devDependencies": {
    "@schibstedspain/sui-multimedia": "0.2",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "copyfiles": "^1.0.0",
    "css-loader": "^0.26.1",
    "eslint": "^3.12.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-config-standard-react": "^4.2.0",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-react": "^6.8.0",
    "eslint-plugin-standard": "^2.0.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "node-sass": "3.4.2",
    "opener": "1.4",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-hot-loader": "^1.3.1",
    "rimraf": "2.5.4",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2",
    "webpack-merge": "^1.1.0"
  },
  "dependencies": {
    "@schibstedspain/sui-spinner": "1.0",
    "react-lazy-load": "3.0.10",
    "imagesloaded": "4.1.1"
  }
}
