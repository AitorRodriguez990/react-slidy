{
  "name": "react-slidy",
  "version": "3.0.28",
  "main": "dist/react-slidy.js",
  "umd:main": "dist/react-slidy.umd.js",
  "module": "dist/react-slidy.mjs",
  "source": "src/react-slidy.js",
  "keywords": [
    "slider",
    "react",
    "lory",
    "slidy",
    "react-lory",
    "react-slidy"
  ],

  "scripts": {
    "build": "microbundle",
    "clean": "rm dist/*",
    "dev": "microbundle watch && open docs/index.html",
    "lint": "sui-lint js"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:miduga/react-slidy.git"
  },
  "peerDependencies": {
    "prop-types": "15",
    "react": "15 || 16"
  },
  "devDependencies": {
    "@s-ui/lint": "2",
    "microbundle": "0.9.0",
    "react": "16.7.0"
  },
  "dependencies": {
    "intersection-observer": "0.5.1"
  },
  "pre-commit": [
    "lint",
    "test",
    "dist"
  ],
  "eslintConfig": {
    "extends": [
      "./node_modules/@s-ui/lint/eslintrc.js",
      "plugin:flowtype/recommended"
    ],
    "plugins": [
      "flowtype"
    ]
  },
  "sasslintConfig": "./node_modules/@s-ui/lint/sass-lint.yml",
  "babel": {
    "presets": [
      "sui",
      "flow"
    ]
  }
}
