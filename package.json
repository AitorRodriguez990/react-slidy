{
  "name": "react-lory",
  "version": "0.0.1",
  "main": "lib/",
  "keywords": [
    "slider",
    "react",
    "lory"
  ],
  "scripts": {
    "clean:lib": "rimraf ./lib/*",
    "clean:dist": "rimraf ./dist/*",
    "predist": "npm run clean:dist -s",
    "dist": "webpack",
    "prelib": "npm run clean:lib -s",
    "lib": "npm run lib:scripts && npm run lib:styles",
    "lib:scripts": "babel src --out-dir lib",
    "lib:styles": "copyfiles -u 1 './src/**/*.scss' lib",
    "prepublish": "npm run lib && npm run dist",
    "lint": "npm run lint:js && npm run lint:sass",
    "lint:eslint": "eslint --ext=.js --ext=.jsx ./src/ ./test/ ./docs/",
    "lint:sass": "node_modules/.bin/sass-lint src/**/*.scss -c -v",
    "start": "npm run start:open && npm run start:server",
    "start:server": "webpack-dev-server",
    "start:open": "opener http://localhost:8080",
    "predoc": "rimraf ./docs/dist/*",
    "doc": "npm run dist && npm run doc:copy && npm run doc:publish",
    "doc:copy": "copyfiles ./dist/* ./docs/",
    "doc:publish": "gh-pages -d docs",
    "phoenix": "rimraf ./node_modules/ && npm install",
    "lint:js": "node_modules/.bin/eslint --ext .js,.jsx src/"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:miduga/react-lory.git"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "peerDependencies": {
    "react": "0.14"
  },
  "devDependencies": {
    "@schibstedspain/frontend-pre-commit-rules": "7.0",
    "@schibstedspain/sui-multimedia": "^0.2.8",
    "babel": "6.1.18",
    "babel-cli": "6.2.0",
    "babel-core": "6.2.1",
    "babel-loader": "6.2.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "6.1.18",
    "babel-preset-react": "6.1.18",
    "copyfiles": "^1.0.0",
    "css-loader": "0.23.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "gh-pages": "0.5.0",
    "node-sass": "3.4.2",
    "opener": "1.4.1",
    "react": "0.14.0",
    "react-dom": "0.14.0",
    "react-hot-loader": "1.3.0",
    "rimraf": "^2.5.4",
    "sass-loader": "3.1.2",
    "style-loader": "0.13.0",
    "webpack": "1.12.14",
    "webpack-dev-server": "1.14.1",
    "webpack-merge": "0.8.3"
  },
  "dependencies": {
    "imagesloaded": "4.1.1",
    "lory.js": "2.2.0"
  }
}
