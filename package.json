{
  "name": "react-slidy",
  "version": "4.0.0-beta.4",
  "main": "lib/",
  "keywords": [
    "slider",
    "react",
    "lory",
    "slidy",
    "react-lory",
    "react-slidy",
    "slider react",
    "react slider"
  ],
  "scripts": {
    "dev": "npm run docz:dev",
    "docz:dev": "docz dev",
    "docz:build": "NODE_ENV=production docz build",
    "clean:lib": "rimraf ./lib/*",
    "lib": "npm run lib:scripts && npm run lib:styles",
    "lib:scripts": "babel src --out-dir lib",
    "lib:styles": "copyfiles -u 1 './src/**/*.scss' lib",
    "lint": "npm run lint:js && npm run lint:sass",
    "lint:js": "sui-lint js",
    "lint:sass": "sui-lint sass",
    "now-build": "npm run docz:build",
    "prelib": "npm run clean:lib -s",
    "prepublish": "npm run lib"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:midudev/react-slidy.git"
  },
  "peerDependencies": {
    "prop-types": "15",
    "react": ">= 16.8.0 < 17",
    "react-dom": ">= 16.8.0 < 17"
  },
  "devDependencies": {
    "@babel/cli": "7",
    "@s-ui/lint": "2",
    "babel-preset-sui": "2",
    "copyfiles": "1.0.0",
    "docz": "0.13.7",
    "docz-plugin-css": "0.11.0",
    "docz-theme-default": "0.13.7",
    "prop-types": "15",
    "react": ">= 16.8.0 < 17",
    "react-dom": ">= 16.8.0 < 17",
    "rimraf": "2.5.4"
  },
  "dependencies": {
    "intersection-observer": "0.5.1"
  },
  "pre-commit": [
    "lint",
    "test",
    "dist"
  ],
  "eslintConfig": {
    "extends": [
      "./node_modules/@s-ui/lint/eslintrc.js"
    ]
  },
  "sasslintConfig": "./node_modules/@s-ui/lint/sass-lint.yml",
  "babel": {
    "presets": [
      "sui"
    ]
  }
}
