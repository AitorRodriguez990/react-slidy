{
  "name": "react-slidy",
  "version": "3.2.0",
  "main": "lib/",
  "keywords": [
    "slider",
    "react",
    "lory",
    "slidy",
    "react-lory",
    "react-slidy"
  ],
  "scripts": {
    "clean:lib": "rimraf ./lib/*",
    "dev": "sui-bundler dev",
    "lib": "npm run lib:scripts && npm run lib:styles",
    "lib:scripts": "babel src --out-dir lib",
    "lib:styles": "copyfiles -u 1 './src/**/*.scss' lib",
    "lint": "npm run lint:js && npm run lint:sass",
    "lint:js": "sui-lint js",
    "lint:sass": "sui-lint sass",
    "now-build": "sui-bundler build -C",
    "prelib": "npm run clean:lib -s",
    "prepublish": "npm run lib"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:miduga/react-slidy.git"
  },
  "peerDependencies": {
    "prop-types": "15",
    "react": ">= 16.8.0 < 17",
    "react-dom": ">= 16.8.0 < 17"
  },
  "devDependencies": {
    "@s-ui/bundler": "4",
    "@s-ui/lint": "2",
    "@babel/cli": "7",
    "babel-preset-sui": "2",
    "copyfiles": "1.0.0",
    "highlight.js": "9.10.0",
    "js-beautify": "1.6.12",
    "jsx-to-string": "1.1.0",
    "node-sass": "4.7.2",
    "opener": "1.4",
    "prop-types": "15",
    "react": ">= 16.8.0 < 17",
    "react-dom": ">= 16.8.0 < 17",
    "react-lowlight": "2.0.0",
    "react-router": "3",
    "react-syntax-highlighter": "5.1",
    "rimraf": "2.5.4"
  },
  "dependencies": {
    "intersection-observer": "0.5.1"
  },
  "pre-commit": [
    "lint",
    "test",
    "dist"
  ],
  "eslintConfig": {
    "extends": [
      "./node_modules/@s-ui/lint/eslintrc.js"
    ]
  },
  "sasslintConfig": "./node_modules/@s-ui/lint/sass-lint.yml",
  "babel": {
    "presets": [
      "sui"
    ]
  }
}
