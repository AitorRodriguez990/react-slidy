{
  "name": "react-slidy",
  "version": "3.0.17",
  "main": "lib/",
  "keywords": [
    "slider",
    "react",
    "lory",
    "slidy",
    "react-lory",
    "react-slidy"
  ],
  "scripts": {
    "clean:lib": "rimraf ./lib/*",
    "deploy": "NODE_ENV=production npm run dist && surge ./docs react-slidy.surge.sh",
    "dist": "webpack",
    "flow": "flow",
    "lib": "npm run lib:scripts && npm run lib:styles",
    "lib:scripts": "babel src --out-dir lib",
    "lib:styles": "copyfiles -u 1 './src/**/*.scss' lib",
    "lint": "npm run lint:js && npm run lint:sass",
    "lint:js": "sui-lint js",
    "lint:sass": "sui-lint sass",
    "prelib": "npm run clean:lib -s",
    "prepublish": "npm run lib",
    "start": "npm run start:open && npm run start:server",
    "start:open": "opener http://localhost:3000",
    "start:server": "webpack-dev-server"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:miduga/react-slidy.git"
  },
  "peerDependencies": {
    "prop-types": "15",
    "react": "0.14 || 15 || 16"
  },
  "devDependencies": {
    "@s-ui/lint": "2",
    "@schibstedspain/sui-multimedia": "0.4",
    "babel-cli": "6",
    "babel-core": "6",
    "babel-loader": "6",
    "babel-preset-flow": "6.23.0",
    "babel-preset-sui": "1",
    "copyfiles": "1.0.0",
    "css-loader": "0.26.1",
    "eslint-plugin-flowtype": "2.35.0",
    "extract-text-webpack-plugin": "1.0.1",
    "flow-bin": "0.51.1",
    "highlight.js": "9.10.0",
    "js-beautify": "1.6.12",
    "jsx-to-string": "1.1.0",
    "node-sass": "3.4.2",
    "opener": "1.4",
    "prop-types": "15",
    "react": "15",
    "react-dom": "15",
    "react-hot-loader": "1.3.1",
    "react-lowlight": "1.0.4",
    "react-router": "2",
    "react-syntax-highlighter": "5.1",
    "rimraf": "2.5.4",
    "sass-loader": "4.0.2",
    "style-loader": "0.13.1",
    "webpack": "1.14.0",
    "webpack-dev-server": "1.16.2",
    "webpack-merge": "1.1.0"
  },
  "dependencies": {
    "@schibstedspain/sui-spinner-basic": "1",
    "intersection-observer": "0.5.0"
  },
  "pre-commit": [
    "lint",
    "test",
    "dist"
  ],
  "eslintConfig": {
    "extends": [
      "./node_modules/@s-ui/lint/eslintrc.js",
      "plugin:flowtype/recommended"
    ],
    "plugins": [
      "flowtype"
    ]
  },
  "sasslintConfig": "./node_modules/@s-ui/lint/sass-lint.yml",
  "babel": {
    "presets": [
      "sui",
      "flow"
    ]
  }
}
